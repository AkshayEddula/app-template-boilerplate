{
    "cli": {
        "version": ">= 16.20.1",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "ios": {
                "simulator": true
            },
            "android": {
                "gradleCommand": ":app:assembleDebug",
                "buildType": "apk"
            }
        },
        "previewAndroid": {
            "android": {
                "buildType": "apk"
            }
        },
        "preview": {
            "distribution": "internal",
            "ios": {
                "simulator": true
            }
        },
        "production": {
            "autoIncrement": true
        },
        "simulator": {
            "ios": {
                "simulator": true
            }
        }
    },
    "submit": {
        "production": {}
    }
}